# Render n8n Environment Sample
N8N_HOST=n8n.<yourdomain.com>
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.<yourdomain.com>/

# Generate: openssl rand -base64 32
N8N_ENCRYPTION_KEY=

# Database auto-wired from Render Postgres (ocs-n8n-db)
# DB_TYPE=postgresdb
# DB_POSTGRESDB_HOST, PORT, DATABASE, USER, PASSWORD auto-set by Render

# Execution cleanup
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=168
