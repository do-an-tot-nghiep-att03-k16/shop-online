# n8n Configuration
N8N_HOST=n8n.aristia.shop
N8N_PORT=5678
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.aristia.shop/

# Generate a random encryption key: openssl rand -base64 32
N8N_ENCRYPTION_KEY=

# PostgreSQL Database for n8n (docker-compose only)
N8N_DB_NAME=n8n
N8N_DB_USER=n8n
N8N_DB_PASSWORD=

# Execution data cleanup (keep last 7 days)
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=168
